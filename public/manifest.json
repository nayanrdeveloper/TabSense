{
    "manifest_version": 3,
    "name": "TabSense",
    "version": "1.0.0",
    "description": "Smart Tab Memory & Focus Optimizer",
    "permissions": [
        "tabs",
        "storage",
        "system.memory",
        "scripting",
        "alarms",
        "tabGroups"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "icons": {}
}